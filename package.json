{
  "name": "share",
  "version": "0.0.1",
  "description": "shared components and utils for the great RMB frontend group",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "doc": "npx jsdoc && node doc/open.js",
    "coverage": "npx jest --coverage && node jest/openCoverage.js"
  },
  "keywords": [],
  "author": "wangfan15@jd.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browserslist": "^4.0.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "jest-environment-enzyme": "^6.0.2",
    "jest-enzyme": "^6.0.2",
    "jsdoc": "^3.5.5",
    "prettier": "^1.14.0"
  },
  "dependencies": {
    "antd": "^3.7.2",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^6.5.2",
    "fxios": "^0.4.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.12.0",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-router": "^4.0.4",
    "moment": "^2.22.2",
    "opn": "^5.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/component/**/*.{js,jsx,mjs}",
      "src/page/**/*.{js,jsx,mjs}",
      "src/store/**/*.{js,jsx,mjs}",
      "src/storeProp/**/*.{js,jsx,mjs}",
      "src/mixin/**/*.{js,jsx,mjs}",
      "src/util/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/__test__/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testEnvironment": "enzyme",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/).+(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^share/(.+)$": "<rootDir>/src/$1",
      "history/createBrowserHistory": "<rootDir>/node_modules/history/createMemoryHistory"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
